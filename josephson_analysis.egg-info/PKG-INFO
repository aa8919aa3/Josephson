    Ic=1.0e-6,          # è‡¨ç•Œé›»æµ 1 Î¼A
    phi_range=(-20e-5, 0),  # ç£é€šç¯„åœ
    n_points=500,
    noise_level=2e-7
)

# åˆ†æé€±æœŸæ€§
